<%= form_for [@box = Box.new] do |f| %>

  <%= f.label :title %>
  <%= f.text_field :title %>

  <%= f.label :description %>
  <%= f.text_field :description %>

  <%= f.label :image_url %>
  <%= f.text_field :image %>

  <div class="actions">
      <%= f.submit "Add new Box", class: "btn btn-success" %>
  </div>

<% end %>
